{
  "name": "simulador-de-antecipacao",
  "version": "1.0.0",
  "description": "Projeto de avaliação para vaga de desenvolvedor Front",
  "scripts": {
    "test": "jest --config ./jest-config.js",
    "build": "webpack --mode=production && node-sass ./src/scss/main.scss ./dist/bundle.css && exit 0",
